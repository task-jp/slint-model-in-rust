import { Rect } from "backend.slint";

export component AppWindow inherits Window {
    title: "Rectangles";
    in property <[Rect]> model;
    for rect[idx] in root.model : item := Rectangle {
        x: rect.x;
	y: rect.y;
	width: rect.width;
	height: rect.height;
	background: rect.color;
	opacity: 0.25;
	border-width: 1px;
	border-color: black;
    }
}
